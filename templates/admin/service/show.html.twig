{% extends 'base.html.twig' %}

{% block title %}Service{% endblock %}

{% block body %}
    <h1>Service</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ service.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ service.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ service.type }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ service.adress }}</td>
            </tr>
            <tr>
                <th>Public</th>
                <td>{{ service.public ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CurrentStatus</th>
                <td>{{ service.currentStatus }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ service.url }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_service_index') }}">back to list</a>

    <a href="{{ path('app_admin_service_edit', {'id': service.id}) }}">edit</a>

    {{ include('admin/service/_delete_form.html.twig') }}
{% endblock %}
